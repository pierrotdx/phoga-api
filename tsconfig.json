{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "types": ["node", "@types/jest"],
    "baseUrl": "./src",
    "paths": {
      "#auth-context": ["./auth-context/index.js"],
      "#logger-context": ["./logger-context/index.js"],
      "#photo-context": ["./photo-context/index.js"],
      "#shared/*": ["./shared/*"]
    }
  },
  "lib": ["es2015"],
  "include": ["src/**/*"]
}
